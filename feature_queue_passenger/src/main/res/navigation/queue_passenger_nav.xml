<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/queue_passenger_nav"
    app:startDestination="@id/queuePassengerFragment">

    <fragment
        android:id="@+id/queuePassengerFragment"
        android:name="id.bluebird.mall.home.QueuePassengerFragment"
        android:label="@string/queue_passenger">
    </fragment>
    <fragment
        android:id="@+id/queueTicket"
        android:name="id.bluebird.mall.home.queue_ticket.QueueTicket"
        android:label="Antrian Tiket">
        <argument
            android:name="queueNumber"
            app:argType="string"/>
        <argument
            android:name="currentNumber"
            app:argType="string"/>
        <argument
            android:name="totalQueue"
            app:argType="string"/>
        <argument
            android:name="createdAt"
            app:argType="string"/>
    </fragment>
    <deepLink app:uri="android-app://id.bluebird.mall/queue_passnger" />
</navigation>
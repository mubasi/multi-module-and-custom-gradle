<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/select_location_nav"
    app:startDestination="@id/selectLocationFragment">
    <fragment
        android:id="@+id/selectLocationFragment"
        android:name="id.bluebird.vsm.feature.select_location.FragmentSelectLocation"
        android:label="SelectLocationFragment">
        <argument
            android:name="isMenuFleet"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/searchLocationFragment"
        android:name="id.bluebird.vsm.feature.select_location.search_mall_location.FragmentSearchMallLocation"
        android:label="@string/search_location_mall">
        <argument
            android:name="parentList"
            app:nullable="false"
            app:argType="id.bluebird.vsm.feature.select_location.model.CacheParentModel[]"/>
        <argument
            android:name="childList"
            app:nullable="false"
            app:argType="id.bluebird.vsm.feature.select_location.model.SubLocation[]"/>
    </fragment>
    <argument android:defaultValue="false"
        app:argType="boolean"
        android:name="isMenu"/>
    <deepLink app:uri="android-app://id.bluebird.mall/select_location/{isMenu}" />

</navigation>
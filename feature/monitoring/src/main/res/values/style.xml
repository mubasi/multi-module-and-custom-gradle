<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="UbuntuRegularGraySubtle">
        <item name="android:fontFamily">@font/ubuntu_regular</item>
        <item name="android:textColor">@color/gray_subtitle</item>
    </style>
    <style name="UbuntuMediumGraySubtle">
        <item name="android:fontFamily">@font/ubuntu_medium</item>
        <item name="android:textColor">@color/gray_subtitle</item>
    </style>
    <style name="UbuntuMediumBlack">
        <item name="android:fontFamily">@font/ubuntu_medium</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="UbuntuBoldWhite">
        <item name="android:fontFamily">@font/ubuntu_bold</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="BackgroundCancel">
        <item name="android:background">@drawable/bg_cancel</item>
        <item name="android:paddingStart">24dp</item>
        <item name="android:paddingEnd">24dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:textAlignment">center</item>
        <item name="fontFamily">@font/ubuntu_bold</item>
        <item name="android:textColor">@color/primary_0</item>
    </style>
    <style name="BottomSheetDialog" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowSoftInputMode">adjustResize|stateVisible</item>
        <item name="android:colorBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
    </style>
</resources>
<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CropImageView">
        <attr name="cropGuidelines">
            <enum name="off" value="0"/>
            <enum name="onTouch" value="1"/>
            <enum name="on" value="2"/>
        </attr>
        <attr name="cropScaleType">
            <enum name="fitCenter" value="0"/>
            <enum name="center" value="1"/>
            <enum name="centerCrop" value="2"/>
            <enum name="centerInside" value="3"/>
        </attr>
        <attr name="cropShape">
            <enum name="rectangle" value="0"/>
            <enum name="oval" value="1"/>
            <enum name="rectangleVerticalOnly" value="2"/>
            <enum name="rectangleHorizontalOnly" value="3"/>
        </attr>
        <attr name="cropAutoZoomEnabled" format="boolean"/>
        <attr name="cropMaxZoom" format="integer"/>
        <attr name="cropMultiTouchEnabled" format="boolean"/>
        <attr name="cropCenterMoveEnabled" format="boolean"/>
        <attr name="cropFixAspectRatio" format="boolean"/>
        <attr name="cropAspectRatioX" format="integer"/>
        <attr name="cropAspectRatioY" format="integer"/>
        <attr name="cropInitialCropWindowPaddingRatio" format="float"/>
        <attr name="cropBorderLineThickness" format="dimension"/>
        <attr name="cropBorderLineColor" format="color"/>
        <attr name="cropBorderCornerThickness" format="dimension"/>
        <attr name="cropBorderCornerOffset" format="dimension"/>
        <attr name="cropBorderCornerLength" format="dimension"/>
        <attr name="cropBorderCornerColor" format="color"/>
        <attr name="cropGuidelinesThickness" format="dimension"/>
        <attr name="cropGuidelinesColor" format="color"/>
        <attr name="cropBackgroundColor" format="color"/>
        <attr name="cropSnapRadius" format="dimension"/>
        <attr name="cropTouchRadius" format="dimension"/>
        <attr name="cropSaveBitmapToInstanceState" format="boolean"/>
        <attr name="cropShowCropOverlay" format="boolean"/>
        <attr name="cropShowProgressBar" format="boolean"/>
        <attr name="cropMinCropWindowWidth" format="dimension"/>
        <attr name="cropMinCropWindowHeight" format="dimension"/>
        <attr name="cropMinCropResultWidthPX" format="float"/>
        <attr name="cropMinCropResultHeightPX" format="float"/>
        <attr name="cropMaxCropResultWidthPX" format="float"/>
        <attr name="cropMaxCropResultHeightPX" format="float"/>
        <attr name="cropFlipHorizontally" format="boolean"/>
        <attr name="cropFlipVertically" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="SlideData">
        <attr name="slideBackground" format="reference|color" />
        <attr name="slideSrc" format="reference" />
        <attr name="slideSrcMargin" format="dimension" />
        <attr name="slideSrcMarginLeft" format="dimension" />
        <attr name="slideSrcMarginRight" format="dimension" />
        <attr name="slideSrcMarginTop" format="dimension" />
        <attr name="slideSrcMarginBottom" format="dimension" />
        <attr name="slideText" format="string" />
        <attr name="slideTextColor" format="color" />
        <attr name="slideTextSize" format="dimension" />
        <attr name="duration" format="integer" />
        <attr name="slideSuccessPercent" format="integer" />
    </declare-styleable>

    <declare-styleable name="PinEntryEditText" >
        <attr name="pinAnimationType" format="enum" >
            <enum name="popIn" value="0" />
            <enum name="fromBottom" value="1" />
            <enum name="none" value="-1" />
        </attr >
        <attr name="pinCharacterMask" format="string" />
        <attr name="pinRepeatedHint" format="string" />
        <attr name="pinLineStroke" format="dimension" />
        <attr name="pinLineStrokeSelected" format="dimension" />
        <attr name="pinCharacterSpacing" format="dimension" />
        <attr name="pinTextBottomPadding" format="dimension" />
        <attr name="pinLineColors" format="color" />
        <attr name="pinBackgroundDrawable" format="reference" />
        <attr name="pinBackgroundIsSquare" format="boolean" />
    </declare-styleable >

    <declare-styleable name="CounterBarView">
        <attr name="titleText" format="string" />
        <attr name="titleColor" format="color" />
        <attr name="counterText" format="string" />
        <attr name="counterStyle" format="enum">
            <enum name="orange" value="0" />
            <enum name="green" value="1" />
            <enum name="black" value="2" />
            <enum name="blue" value="3" />
            <enum name="blue_alt" value="4" />
            <enum name="yellow" value="5" />
        </attr>
        <attr name="plusIconVisible" format="boolean" />
        <attr name="plusIconText" format="string" />
        <attr name="additionalDataVisible" format="boolean" />
        <attr name="secondaryAdditionalDataVisible" format="boolean" />
        <attr name="forceShowAdditionalDataLocation" format="boolean" />
        <attr name="highestRequestCount" format="integer" />
        <attr name="highestRequestLocation" format="string" />
        <attr name="secondHighestRequestCount" format="integer" />
        <attr name="secondHighestRequestLocation" format="string" />
    </declare-styleable>


</resources>